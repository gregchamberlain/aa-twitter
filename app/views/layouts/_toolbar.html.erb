<navbar class="toolbar">
  <ul>
    <li><%= link_to "Feed", feed_url %></li>
  </ul>

  <nav class="users-search">
    <input type="text" name="name" value="" placeholder="Search">
    <i class="fa fa-search"></i>
    <ul>

    </ul>
  </nav>
  <h3 class="logo">TWITTER 2.0</h3>
  <% if current_user %>
    <form action="/session" method="POST">
      <input type="hidden" name="_method" value="DELETE">
      <input
        type="hidden"
        name="authenticity_token"
        value="<%= form_authenticity_token %>">

      <input type="submit" value="Logout!" class="post-tweet">
    </form>
  <% end %>
</navbar>
